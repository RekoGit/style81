{
  "name": "style81",
  "description": "81道場の棋譜再生画面を見やすくスタイルします",
  "version": "1.0.0",
  "manifest_version": 3,
  "icons": {
    "128": "icon.png"
  },
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://system.81dojo.com/ja/kifus/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ]
}